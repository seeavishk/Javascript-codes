let x=[1,3,4,"aviral","shu"]
let b=x.toString() //will print the elements in the form of string only
console.log(x)
console.log("change the value")
console.log(b)
//join,pop,push methods are more methods in this
x.pop()
console.log(x)  //LAST WAALA NIKAL DEGA FROM THE ARRAY
x.push(8)
console.log(x)
//SHIFT method is used to remove the very  first element of the string
x.shift()
console.log(x)
